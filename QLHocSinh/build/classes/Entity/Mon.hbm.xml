<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

  <class name="Entity.Mon" table="mon" >
        <id name="id" type="int">
        <column name="id" length="11" />
        <generator class="assigned" />
        </id>
        <property name="tenmon" type="string">
        <column name="tenmon" length="255" />
        </property> 
         <property name="hesomon" type="string">
        <column name="hesomon" length="255" />
        </property>        
         <property name="mamon" type="string">
        <column name="mamon" length="255" />
        </property>  
    </class>

</hibernate-mapping>

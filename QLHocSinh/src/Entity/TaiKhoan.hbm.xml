<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="Entity.TaiKhoan" table="taikhoan" catalog="quanlyhocsinh">
        <id name="id" type="int">
        <column name="id" length="11" />
        <generator class="assigned" />
        </id>
        <property name="tentaikhoan" type="string">
        <column name="tentaikhoan" length="255" />
        </property>
         <property name="matkhau" type="string">
        <column name="matkhau" length="255" />
        </property>
         <property name="chucvu" type="int">
        <column name="chucvu" length="11" />
        </property>
    </class>
  

</hibernate-mapping>

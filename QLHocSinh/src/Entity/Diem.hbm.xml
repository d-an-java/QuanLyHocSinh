<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

   <class name="Entity.Diem" table="diem" >
        <id name="id" type="int">
        <column name="id" length="11" />
        <generator class="assigned" />
        </id>
        <property name="malop" type="string">
        <column name="malop" length="255" />
        </property> 
        <property name="mahocsinh" type="string">
        <column name="mahocsinh" length="255" />
        </property>  
        <property name="mamonhoc" type="string">
        <column name="mamonhoc" length="255" />
        </property>        
         <property name="mahocky" type="string">
        <column name="mahocky" length="255" />
        </property> 
         <property name="diemtbmon" type="float">
        <column name="diemtbmon" length="10" />
        </property>
           <property name="diemheso1" type="float">
        <column name="diemheso1" length="10" />
        </property>
         <property name="diemheso2" type="float">
        <column name="diemheso2" length="10" />
        </property>
         <property name="diemheso3" type="float">
        <column name="diemheso3" length="10" />
        </property>
    </class>

</hibernate-mapping>
